{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/index.html",
          "/**.css",
          "/**.js",
          "/favicons/**",
          "/manifest.webmanifest"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "data",
      "urls": [
        "/data/**.txt"
      ],
      "cacheConfig": {
        "maxSize": 10,
        "maxAge": "2d",
        "strategy": "freshness",
        "timeout": "5s"
      }
    }
  ]
}